Notification {
    color: #000000;
    background: #c7c7c7;
    border: 1px solid #9d9d9d;
    border-radius: 2px;
}

QLabel {
    color: #000000;
}

#appLabel {
    font-weight: bold;
}

#closeButton {
    color: #000000;
    background: none;
    border: 4px solid transparent;
    border-image: none;
    margin: 0px;
    padding: 0px;
}

#closeButton:hover {
    border-image: url(assets/buttons/button.png);
}

#closeButton:pressed {
    border-image: url(assets/buttons/active.png);
}
